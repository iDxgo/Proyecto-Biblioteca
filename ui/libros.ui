  <property name="styleSheet">
   <string notr="true">/* === Estilo general - Tema Oscuro Biblioteca === */
QMainWindow {
    background-color: #121212;
    color: #e0e0e0;
    font-family: &quot;Segoe UI&quot;, &quot;SF Pro Display&quot;, &quot;Roboto&quot;, sans-serif;
    font-size: 14px;
    font-weight: 400;
}

/* === Frames === */
QFrame {
    background-color: #1e1e1e;
    border-radius: 8px;
    border: 1px solid #2d2d2d;
}

/* Barra superior */
#frame_superior {
    background-color: #1a1a1a;
    border: none;
    border-radius: 0px;
    border-bottom: 2px solid #7b1fa2;
}

/* Panel lateral */
#frame_lateral {
    background-color: #1a1a1a;
    border-right: 2px solid #7b1fa2;
    border-radius: 0px;
}

/* Área principal */
#frame_paginas {
    background-color: #1e1e1e;
    border-radius: 0px;
}

/* === Botones === */
QPushButton {
    background-color: #4a148c;
    color: #ffffff;
    border: 1px solid #6a1b9a;
    border-radius: 6px;
    padding: 10px 16px;
    font-weight: 500;
    font-size: 14px;
    letter-spacing: 0.5px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #7b1fa2, stop:1 #4a148c);
}

QPushButton:hover {
    background-color: #6a1b9a;
    color: #ffffff;
    border: 1px solid #8e24aa;
}

QPushButton:pressed {
    background-color: #38006b;
    color: #ffffff;
    border: 1px solid #4a148c;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #4a148c, stop:1 #38006b);
}

/* Botón principal (Menú) */
#boton_menu {
    background-color: #7b1fa2;
    color: white;
    border: none;
    font-weight: 600;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #9c27b0, stop:1 #7b1fa2);
}
#boton_menu:hover {
    background-color: #8e24aa;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #ab47bc, stop:1 #8e24aa);
}
#boton_menu:pressed {
    background-color: #6a1b9a;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #7b1fa2, stop:1 #6a1b9a);
}

/* Botón salir */
#boton_salir {
    background-color: #c62828;
    color: white;
    border: none;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #ef5350, stop:1 #b71c1c);
}
#boton_salir:hover {
    background-color: #d32f2f;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #f44336, stop:1 #c62828);
}
#boton_salir:pressed {
    background-color: #b71c1c;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 #c62828, stop:1 #b71c1c);
}

/* === Campos de texto === */
QLineEdit {
    background-color: #2d2d2d;
    color: #e0e0e0;
    border: 1px solid #424242;
    border-radius: 4px;
    padding: 8px 12px;
    font-size: 14px;
    font-weight: 400;
    selection-background-color: #7b1fa2;
}

QLineEdit:focus {
    border: 1px solid #7b1fa2;
    background-color: #333333;
    box-shadow: 0px 0px 5px rgba(123, 31, 162, 0.5);
}

QLineEdit:placeholder {
    color: #666666;
}

/* === Labels === */
QLabel {
    color: #bdbdbd;
    font-weight: 500;
    font-size: 14px;
    border: none;
}

/* Label inferior o secundario */
#label_mensaje {
    color: #9e9e9e;
    font-size: 10px;
    font-style: italic;
}

/* === Stacked Widget === */
QStackedWidget {
    background-color: #1e1e1e;
    border: 1px solid #2d2d2d;
    border-radius: 8px;
}

/* === Scrollbars === */
QScrollBar:vertical {
    border: none;
    background: #2d2d2d;
    width: 12px;
    border-radius: 6px;
}
QScrollBar::handle:vertical {
    background: #4a148c;
    min-height: 25px;
    border-radius: 6px;
}
QScrollBar::handle:vertical:hover {
    background: #6a1b9a;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}

QScrollBar:horizontal {
    border: none;
    background: #2d2d2d;
    height: 12px;
    border-radius: 6px;
}
QScrollBar::handle:horizontal {
    background: #4a148c;
    min-width: 25px;
    border-radius: 6px;
}
QScrollBar::handle:horizontal:hover {
    background: #6a1b9a;
}

/* === Layouts === */
QVBoxLayout, QHBoxLayout {
    spacing: 12px;
}

/* === Botones laterales === */
#boton_crear, #boton_buscar, #boton_actualizar, #boton_eliminar, #boton_consultar_todos {
    background-color: #2d2d2d;
    border: 1px solid #424242;
    color: #bdbdbd;
    font-weight: 500;
    font-size: 14px;
    text-align: left;
    padding: 12px 16px;
}

#boton_crear:hover, 
#boton_buscar:hover, 
#boton_actualizar:hover, 
#boton_eliminar:hover,
#boton_consultar_todos:hover {
    background-color: #333333;
    border: 1px solid #7b1fa2;
    color: #e0e0e0;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #2d2d2d, stop:1 #4a148c);
}

#boton_crear:pressed, 
#boton_buscar:pressed, 
#boton_actualizar:pressed, 
#boton_eliminar:pressed,
#boton_consultar_todos:pressed {
    background-color: #4a148c;
    border: 1px solid #6a1b9a;
    color: #ffffff;
}

/* === Tooltips === */
QToolTip {
    background-color: #2d2d2d;
    color: #e0e0e0;
    border: 1px solid #7b1fa2;
    padding: 8px;
    border-radius: 6px;
    font-size: 13px;
    font-weight: 500;
    box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.3);
}

/* === Tablas === */
QTableWidget {
    background-color: #1e1e1e;
    color: #e0e0e0;
    gridline-color: #424242;
    border: 1px solid #2d2d2d;
    border-radius: 6px;
    font-size: 13px;
    font-weight: 400;
    selection-background-color: #7b1fa2;
    selection-color: #ffffff;
    alternate-background-color: #252525;
}

QHeaderView::section {
    background-color: #4a148c;
    color: #ffffff;
    border: 1px solid #6a1b9a;
    border-radius: 4px;
    font-weight: 600;
    font-size: 13px;
    padding: 8px;
}

QHeaderView::section:hover {
    background-color: #6a1b9a;
}

QTableWidget::item {
    padding: 6px;
    border: none;
}

QTableWidget::item:selected {
    background-color: #7b1fa2;
    color: #ffffff;
}

/* Efectos adicionales para profundidad */
#frame_superior {
    border-bottom: 2px solid #7b1fa2;
}

#frame_lateral {
    border-right: 2px solid #7b1fa2;
}

/* Efectos de sombra suaves */
QFrame {
    box-shadow: 0px 2px 8px rgba(0, 0, 0, 0.2);
}

QPushButton {
    box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.3);
}

QLineEdit:focus {
    box-shadow: 0px 0px 8px rgba(123, 31, 162, 0.4);
}</string>
